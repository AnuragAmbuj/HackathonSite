<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ProjectKnight</servlet-name>
		<servlet-class>club.decoders.hackathon.ProjectKnightServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectKnight</servlet-name>
		<url-pattern>/projectknight</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegServlet</servlet-name>
		<servlet-class>club.decoders.hackathon.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegServlet</servlet-name>
		<url-pattern>/registrationservlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>IdeaServlet</servlet-name>
		<servlet-class>club.decoders.hackathon.IdeaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IdeaServlet</servlet-name>
		<url-pattern>/masthandler</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AnnServlet</servlet-name>
		<servlet-class>club.decoders.hackathon.AnnouncementHandlerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AnnServlet</servlet-name>
		<url-pattern>/announcementhandlerservlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TeamHandler</servlet-name>
		<servlet-class>club.decoders.hackathon.TeamHandlerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TeamHandler</servlet-name>
		<url-pattern>/teammaker</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
